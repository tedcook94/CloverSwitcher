        <Button fx:id="cancelButton" alignment="CENTER" contentDisplay="CENTER" layoutX="15.0" layoutY="160.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#cancelEntry" prefHeight="25.0" prefWidth="75.0" text="Cancel" />
